# Discord Bot 設定
DISCORD_TOKEN=your_discord_bot_token
DISCORD_CLIENT_ID=your_client_id
DISCORD_GUILD_ID=your_guild_id

# 監控設定 (可選)
CHECK_INTERVAL=60000          # 監控間隔（毫秒），預設 60 秒
PER_FETCH_DELAY=2000          # 每次抓取延遲（毫秒），預設 2 秒

# 重試設定 (可選)
RETRY_MAX_ATTEMPTS=3          # 最大重試次數，預設 3 次
RETRY_INITIAL_DELAY=1000      # 初始重試延遲（毫秒），預設 1 秒
RETRY_MAX_DELAY=10000         # 最大重試延遲（毫秒），預設 10 秒

# 清理設定 (可選)
TMP_DATA_AGE_DAYS=7           # 暫存資料保留天數，預設 7 天
CLEANUP_INTERVAL_HOURS=24     # 清理間隔（小時），預設 24 小時
